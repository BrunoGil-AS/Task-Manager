{
  "name": "task-manager",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc -p .",
    "watch": "tsc -p . --watch",
    "exec": "node ./dist/index.js",
    "fast": "npm run build && npm run exec",
    "dev": "nodemon --watch src --ext ts --exec \"npm run build && node ./dist/index.js\"",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --config jest.config.cjs",
    "setup": "npm install typescript --save-dev && npx tsc --init && mkdir src && echo // Your TypeScript code goes here > src/index.ts",
    "node-setup": "npm install typescript --save-dev @types/node nodemon express @types/express && npx tsc --init && mkdir src && echo // Your TypeScript + Express code goes here > src/index.ts"
  },
  "keywords": [
    "miniproject"
  ],
  "author": "Bruno Gil R.",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.10",
    "@types/node": "^25.1.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.11",
    "supabase": "^2.72.9",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.93.2",
    "compression": "^1.8.1",
    "cors": "^2.8.6",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "express-validator": "^7.3.1",
    "helmet": "^8.1.0",
    "jose": "^6.1.3",
    "pino": "^10.3.0",
    "pino-pretty": "^13.1.3",
    "postgres": "^3.4.8",
    "zod": "^4.3.6"
  }
}
